y = Total,
fill = Municipio,
label = Total,
text = paste("Municipio:", Municipio, "<br>",
"Total:", Total))) +
geom_point(stat = "identity", show.legend = FALSE, position = "dodge") +
geom_text(position = position_dodge(width = 1), vjust = -0.5) +
theme_classic()+
coord_flip()
ggplotly(plot3, tooltip = "text") %>% layout (showlegend = FALSE)
